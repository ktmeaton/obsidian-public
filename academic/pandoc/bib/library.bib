@article{kazak2016BrucellosisRetrospectiveEvaluation,
  title = {Brucellosis: A Retrospective Evaluation of 164 Cases},
  shorttitle = {Brucellosis},
  author = {Kazak, E and Akalın, H and Yılmaz, E and Heper, Y and Mıstık, R and Sınırtaş, M and Özakın, C and Göral, G and Helvacı, S},
  date = {2016-11},
  journaltitle = {Singapore Medical Journal},
  volume = {57},
