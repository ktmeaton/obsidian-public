@article{searle2009CelticFringeBritain,
  title = {The {{Celtic}} Fringe of {{Britain}}: Insights from Small Mammal Phylogeography},
  shorttitle = {The {{Celtic}} Fringe of {{Britain}}},
  author = {Searle, Jeremy B. and Kotlík, Petr and Rambau, Ramugondo V. and Marková, Silvia and Herman, Jeremy S. and McDevitt, Allan D.},
