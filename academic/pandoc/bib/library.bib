@inproceedings{bakanidze2010SurveillancePlagueNatural,
  title = {Surveillance on {{Plague}} in {{Natural Foci}} in {{Georgia}}},
  booktitle = {Emerging and {{Endemic Pathogens}}},
  author = {Bakanidze, Lela and Imnadze, Paata and Chubinidze, Svetlana and Tsertsvadze, Nikoloz and Mgeladze, Gela and Shalutashvili, Irakli and Tsanava, Shota and Shavishvili, Merab and Manvelyan, Julietta and Ninashvili, Nana and Katsitadze, Guram},
