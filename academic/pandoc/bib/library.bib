@article{li2009GenotypingPhylogeneticAnalysis,
  title = {Genotyping and Phylogenetic Analysis of {{{\emph{Yersinia}}}}{\emph{ Pestis}} by {{MLVA}}: Insights into the Worldwide Expansion of {{Central Asia}} Plague Foci},
  shorttitle = {Genotyping and {{Phylogenetic Analysis}} of {{Yersinia}} Pestis by {{MLVA}}},
  author = {Li, Yanjun and Cui, Yujun and Hauck, Yolande and Platonov, Mikhail E. and Dai, Erhei and Song, Yajun and Guo, Zhaobiao and Pourcel, Christine and Dentovskaya, Svetlana V. and Anisimov, Andrey P. and Yang, Ruifu and Vergnaud, Gilles},
  date = {2009-06-22},
  journaltitle = {PLOS ONE},
  shortjournal = {PLOS ONE},
  volume = {4},
  number = {6},
  pages = {e6000},
  publisher = {{Public Library of Science}},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0006000},
  url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0006000},
  urldate = {2021-05-26},
  abstract = {Background The species Yersinia pestis is commonly divided into three classical biovars, Antiqua, Medievalis, and Orientalis, belonging to subspecies pestis pathogenic for human and the (atypical) non-human pathogenic biovar Microtus (alias Pestoides) including several non-pestis subspecies. Recent progress in molecular typing methods enables large-scale investigations in the population structure of this species. It is now possible to test hypotheses about its evolution which were proposed decades ago. For instance the three classical biovars of different geographical distributions were suggested to originate from Central Asia. Most investigations so far have focused on the typical pestis subspecies representatives found outside of China, whereas the understanding of the emergence of this human pathogen requires the investigation of strains belonging to subspecies pestis from China and to the Microtus biovar. Methodology/Principal Findings Multi-locus VNTR analysis (MLVA) with 25 loci was performed on a collection of Y. pestis isolates originating from the majority of the known foci worldwide and including typical rhamnose-negative subspecies pestis as well as rhamnose-positive subspecies pestis and biovar Microtus. More than 500 isolates from China, the Former Soviet Union (FSU), Mongolia and a number of other foci around the world were characterized and resolved into 350 different genotypes. The data revealed very close relationships existing between some isolates from widely separated foci as well as very high diversity which can conversely be observed between nearby foci. Conclusions/Significance The results obtained are in full agreement with the view that the Y. pestis subsp. pestis pathogenic for humans emerged in the Central Asia region between China, Kazakhstan, Russia and Mongolia, only three clones of which spread out of Central Asia. The relationships among the strains in China, Central Asia and the rest of the world based on the MLVA25 assay provide an unprecedented view on the expansion and microevolution of Y. pestis.},
  langid = {english},
  keywords = {Asia,Bacterial pathogens,China,Genomics,Mongolia,Pandemics,Plagues,Yersinia pestis},
  file = {C\:\\Users\\user5028384\\Zotero\\storage\\NVFP9M9U\\Li et al. - 2009 - Genotyping and Phylogenetic Analysis of Yersinia p.pdf;C\:\\Users\\user5028384\\Zotero\\storage\\VANBGPN6\\article.html}
}

