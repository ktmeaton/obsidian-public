@article{seifert2016GenotypingYersiniaPestis,
  title = {Genotyping {{Yersinia}} Pestis in {{Historical Plague}}: {{Evidence}} for {{Long-Term Persistence}} of {{Y}}. Pestis in {{Europe}} from the 14th to the 17th {{Century}}},
