@article{lam2021ImpactsLowDiversity,
  title = {The Impacts of Low Diversity Sequence Data on Phylodynamic Inference during an Emerging Epidemic},
  author = {Lam, Anthony and Duchene, Sebastian},
  date = {2021-01},
  journaltitle = {Viruses},
  volume = {13},
  number = {1},
  pages = {79},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  doi = {10.3390/v13010079},
  url = {https://www.mdpi.com/1999-4915/13/1/79},
  urldate = {2021-12-02},
  abstract = {Phylodynamic inference is a pivotal tool in understanding transmission dynamics of viral outbreaks. These analyses are strongly guided by the input of an epidemiological model as well as sequence data that must contain sufficient intersequence variability in order to be informative. These criteria, however, may not be met during the early stages of an outbreak. Here we investigate the impact of low diversity sequence data on phylodynamic inference using the birth\&ndash;death and coalescent exponential models. Through our simulation study, estimating the molecular evolutionary rate required enough sequence diversity and is an essential first step for any phylodynamic inference. Following this, the birth\&ndash;death model outperforms the coalescent exponential model in estimating epidemiological parameters, when faced with low diversity sequence data due to explicitly exploiting the sampling times. In contrast, the coalescent model requires additional samples and therefore variability in sequence data before accurate estimates can be obtained. These findings were also supported through our empirical data analyses of an Australian and a New Zealand cluster outbreaks of SARS-CoV-2. Overall, the birth\&ndash;death model is more robust when applied to datasets with low sequence diversity given sampling is specified and this should be considered for future viral outbreak investigations.},
  issue = {1},
  langid = {english},
  keywords = {Bayesian phylogenetics,birth–death,coalescent,phylodynamics,SARS-CoV-2},
  file = {C\:\\Users\\user5028384\\Zotero\\storage\\UDCPXISS\\Lam and Duchene - 2021 - The Impacts of Low Diversity Sequence Data on Phyl.pdf;C\:\\Users\\user5028384\\Zotero\\storage\\488SJL97\\79.html}
}

@thesis{laudisoit2009DIVERSITYECOLOGYSTATUS,
  title = {{{DIVERSITY}}, {{ECOLOGY AND STATUS OF POTENTIAL HOSTS AND VECTORS OF THE PLAGUE BACILLUS YERSINIA PESTIS}}. {{CONTRIBUTION TO PLAGUE EPIDEMIOLOGY IN AN ENDEMIC PLAGUE FOCUS}}: {{THE LUSHOTO DISTRICT}} ({{TANZANIA}}).},
  shorttitle = {{{DIVERSITY}}, {{ECOLOGY AND STATUS OF POTENTIAL HOSTS AND VECTORS OF THE PLAGUE BACILLUS YERSINIA PESTIS}}. {{CONTRIBUTION TO PLAGUE EPIDEMIOLOGY IN AN ENDEMIC PLAGUE FOCUS}}},
  author = {Laudisoit, Anne},
  date = {2009-11-24},
  doi = {10.13140/RG.2.2.25362.25281},
  abstract = {In the Lushoto plague focus, Tanzania, plague - caused by the gramnegative bacterium Yersinia pestis - has been endemic for many years. In 1980, plague emerged in two villages and subsequently spread to more than 50 villages in the following years. Between 1980 and 2004 - when the last cases were reported - 7603 people had been infected by the disease, mostly of the bubonic flea-driven type. During this period, rodent and flea control measures were taken, medical staff trained, a community education program on risks of plague initiated and a Permanent Plague Control Team (PPCT) created and functional. However, plague was never isolated by direct culture from any small mammal species during the 16 years epidemiological monitoring of the region; only positive serology indicated that the plague bacterium circulated among wild rodents. Nonetheless, plague came back twice a year, usually during the rainy seasons. Indeed, until 2004, in spite of rodent and flea management campaigns, plague reappeared in October/November and May/June. Even though plague re-emergence seems linked with climatic factors to some extent, the number of plague cases is prone to an extreme interanual and interlocality variation and seems unpredictable. However, during these last four years research, no plague cases were reported to the health authorities in the district; neither rodents, nor fleas were found positive for plague and none of the above listed factors seemed to contribute alone to the observed plague pattern. Indeed, despite 20 years of epidemiological and ecological research, the relations between the biotic and abiotic factors that favour plague maintenance, and which account for the observed seasonal and spatial distribution of the human cases are still poorly understood. One major reason - non exclusive and clearly understandable - to this gap of knowledge is, to some extent, due to the methodology.Indeed, the emergency response during epidemics and the lack of systematic comparison between the potential hosts and vectors of plague in plagueendemic and plague-free locations, especially before and after human epidemics, does not grant a solid base for a global comprehension of the plague cycle dynamics in the long run. Due to obvious financial constraints, rodent and flea surveys always occurred during (or shortly after) human epidemics, mostly in the domestic and peri-domestic environments. However, times of epidemics or epizootics are not suitable to identify the true reservoir of the plague bacillus. Indeed, during epizootics, the circulation of the plague bacillus is expected in a large variety of mammals since the probability that many different species will have been exposed is real, and hence is likely to hide the true reservoir of the disease. On the other hand, during inter-epidemics and inter-epizootics, the chances are limited to find out the reservoir since firstly, no geographical information allows for the localization of resistant mammals that might be confined to a small area, and second their populations might either be dispersed or less abundant. Consequently, the lack of evidence of the presence of the plague bacillus or evidence of it circulating in the area does not mean that plague has totally disappeared from the area and, as much the true reservoir of the disease as well as the place – outside the domestic environment - where humans acquire the infection remain unknown. The temporary absence or quiescence of Yersinia pestis in Lushoto, and in other endemic foci, is a typical feature of current plague foci that suggests that other yet unknown or under-estimated parameters linked with the different elements of the natural plague cycle might play an important role in the long term maintenance of the bacteria locally. Those parameters might be related to the population dynamics of resistant and susceptible host species, to their associated flea fauna and their seasonal and spatial dynamics, to the vectorial capacity of fleas, to sociocultural factors, to interactions with other sympatric/syntopic bacteria and to the involvement of soil as a biotic reservoir among others.Those factors, and others, are discussed in this dissertation and further highlight how much the classical natural plague cycle might be more complex than historically described, opening new perspectives of research that involve soil microorganisms as intermediate hosts. In 2005, this study was initiated with the main objective of identifying the small mammal and flea species potentially involved in the plague cycle by comparing these animal communities in 2 plague-endemic and 2 neighbouring plague-free locations in order to better understand the ecology of the disease which would – in terms - facilitate the introduction of sustainable strategies for prevention of outbreaks and plague control in case they occur. The secondary objectives underlying this research were first to identify those small mammal and flea species that were only found in plague endemic-locations compared to plaguefree locations, second to monitor plague prevalence (direct and indirect or serological prevalence) and third to investigate the seasonal and spatial population dynamics of the domestic and wild (or sylvatic) small mammal populations. Small mammals were regularly trapped in various habitats (crop field, natural forest, bushes, shrubs and houses) by using different kinds of live traps while domestic fleas were collected using kerosene lamps hung at night in the typical mud houses of the area. In the course of 2006, eight additional villages (3 plague-free and 5 plague-endemic locations) were included in the domestic flea study since preliminary results pointed towards a potential role of the human flea Pulex irritans in plague epidemiology. The socio-cultural issues were also examined by questionnaire targeting household composition, sleeping habits, cattle husbandry, rat and flea nuisance, daily activities and agricultural practices. The major results are presented in seven manuscripts that are split in sections : the domestic plague cycle and the sylvatic plague cycle. In the first section, we show that Pulex irritans, the human flea, is the predominant flea species (72.4\%) in houses of the Lushoto district.Other species collected were Echidnophaga gallinacea (15.1\%), Ctenocephalides felis and C. canis (6.5\%), Xenopsylla brasiliensis (3.4\%), and Tunga penetrans (2.6\%). We show that twice as many houses were infested by P. irritans in high plague frequency villages than in low plague frequency villages. For all the trapping sessions in 2005 and 2006, the P. irritans index (Pii) was indeed 2-9 times greater in plague endemic locations than in plague free locations. While a mere – but strongly positive - correlation was established between the current P. irritans index and the plague frequency and incidence (log transformed) calculated on the past 16 years epidemiological data, these observations suggest that in Lushoto district human fleas may play a role in plague epidemiology. In another active plague focus of Eastern Africa, the Ituri plague focus (DRCongo), none of the domestic fleas analyzed (782/2432) were pestiferous 6 months after an important epidemic outbreak having involved more than 600 suspected cases. The fleas did however carry other flea- and rodent-borne zoonotic pathogens, namely Bartonella vinsonii, B. clarridgeiae, B. rochalimae and Rickettsia felis with an estimated infection rate for R. felis of 10.72\% and for Bartonella spp. of 3.66\%. This is significant in the view that co-infection might play a role in sensitizing rodents and/or humans to other diseases by lowering immune-defences in a population having an already fairly unbalanced diet. We also describe a new technique that might be useful in interpreting flea host switching and how plague might be brought back from endemic pockets into the domestic environment by wild rodent fleas feeding on alternative domestic host. Indeed, we have developed a multiplexed, real-time polymerase chain reaction assay targeting mitochondrial genes that is capable of detecting blood meal sources in fleas collected off-host in East Africa. Future application of the assay will help us better define the ecology of plague in East Africa by providing valuable information regarding the potential flea hosts present in and around human habitations which in combination with the PCRs targeting specific pathogens fragments will allow us to estimate the level of human risk to flea or other vector borne diseases.During the exploration of the sylvatic cycle, we discovered two new flea species that were combed off wild rodents and that add to the list of the potential plague vectors and carriers in the area, namely Ctenophthalmus (Ethioctenophthalmus) teucqae n.sp. and Ctenophthalmus (Ethioctenophthalmus) kemmelberg n. sp. A new subspecies, C.(E.) teucqae shumeensis n.ssp., was also determined following examination of collection and comparison specimen from the National History Museum of London. The second part of the sylvatic cycle section examines the small mammal and flea communities, their diversity, their ecological characteristics, their associations and dynamics in relation with habitat characteristics in general and in plague endemic (PEL) and plague free locations (PFL) in particular. Indeed, the continuous reappearance of the disease in some locations might be due to variations in small mammals and fleas species composition, diversity and relative abundances in plague-endemic and plague-free locations. In particular, the plague pattern might be partly explained by the absence of one or several potential good reservoir(s) and/or vector(s) in plague free locations and the presence of such organisms in plague endemic locations. For these reasons in this part, we first investigate the host and flea species diversity and composition across different habitats (crop fields, natural forest, plantations, open and close shrubbery) during the dry and rainy seasons in the Lushoto district. The paper shows that the only host and flea community parameters affected by either season or habitat type are host density and mean infracommunity flea species richness (species diversity on a particular host species). In particular, host density was lowest in crop fields and natural forest, higher in commercial plantations and open shrubbery, and highest in closed shrubbery. Host individuals harbored, on average, significantly more flea species during the dry season than during the rainy season. We also found that during both seasons, the similarity in flea species composition increased with an increase in the similarity in host species composition.Ordination of habitats according to their host and flea species composition demonstrated that the pattern of between-habitat similarity in both host and flea species composition varied seasonally. Moreover, despite the relatively rich mammal and flea fauna of the study region, the major contribution to variation in species composition between seasons and among habitats was due to a few species only. Indeed, seasonal differences in host species composition within a habitat were mainly due to changes in relative abundance of Lophuromys kilonzoi, Grammomys sp. and Praomys delectorum. whereas seasonal differences in flea species composition were mainly due to changes in relative abundances of Nosopsyllus incisus, Ctenophthalmus eximius and Dinopsyllus grypurus. Moreover, in different hosts, the main differences in species composition of flea assemblages were due to different flea species. In other words, this confirms that flea dynamics is influenced by host diversity and population dynamics and abiotic factors of the off-host environment however the former appears to explain variance in flea species composition between localities in the tropics better than between localities in temperate and arid zones. This analysis also suggests that some flea species are more opportunistic than other, and that host switching is a frequent phenomenon influenced by host population dynamics and habitat. In order to understand the reasons for the geographic and seasonal reemergence of plague, we further characterized host species by species richness and degree of taxonomic relatedness of its flea assemblages. In total, we included 1283 individual small mammals belonging to 20 species from which 2809 individual fleas belonging to 24 species were collected. We found no significant difference in either host density or any variable related to flea abundance or diversity. However, there was a marginally significant effect of taxonomic distinctness of a flea assemblage harbored by a host on its probability to be more abundant in either plague endemic or plaguefree locations. Furthermore, hosts more abundant in plague-endemic locationstended to harbor closely-related fleas. - - 20 - - The relative distribution of some flea species was seasonally opposite between plague-endemic and plague-free locations. In other words, a flea could be more abundant in plague-endemic locations during the dry season, but more abundant in plague-free locations during the rainy season and vice versa. Finally, while opportunistic and specialist fleas were equally distributed in both sets of locations, fleas exploiting distantly-related hosts were found mainly in plague-free locations during the rainy season. We suggest that the observed patterns might arise due to seasonal and spatial differences in local microclimatic conditions and landscape connectivity. Several other factors that could play a role in plague maintenance, re-emergence or transmission were examined by parallel research by master’s students. These included studies on land use patterns and soil composition (2005-2006), landscape connectivity (2006), climatic data collection (2007-2008), flea proventriculus morphology structure (2007), and burrow flea collection (2008). However, these data show that the biotic and abiotic factors - considered in this study - alone cannot explain the human plague pattern in the Lushoto district and that other components (especially soil physical and chemical properties, landscape connectivity, land use pattern, subterranean ecosystems), their multiscale interactions, and the dynamics of these metapopulation at a local and microlocal level have to be integrated in order to better understand plague maintenance and reemergence.},
  file = {C\:\\Users\\user5028384\\Zotero\\storage\\57RRNXKJ\\Laudisoit - 2009 - DIVERSITY, ECOLOGY AND STATUS OF POTENTIAL HOSTS A.pdf}
}

