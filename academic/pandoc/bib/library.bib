  keywords = {climate change,global transmission velocity,historical and genomic data,Third Pandemic,Yersinia pestis},
  file = {C\:\\Users\\user5028384\\Zotero\\storage\\87DBLZWH\\Xu et al. - 2019 - Historical and genomic data reveal the influencing.pdf;C\:\\Users\\user5028384\\Zotero\\storage\\T9NM4SYU\\11833.html}
