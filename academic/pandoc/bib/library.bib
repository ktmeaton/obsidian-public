@article{slavin2021OutWestFormation,
  title = {Out of the {{West}}: Formation of a Permanent Plague Reservoir in South-Central {{Germany}} (1349–1356) and Its Implications},
  shorttitle = {Out of the {{West}}},
  author = {Slavin, Philip},
  date = {2021},
  journaltitle = {Past \& Present},
