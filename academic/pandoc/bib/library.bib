@article{rasmussen2015EarlyDivergentStrains,
  title = {Early Divergent Strains of {{{\emph{Yersinia}}}}{\emph{ Pestis}} in {{Eurasia}} 5,000 Years Ago},
  author = {Rasmussen, Simon and Allentoft, Morten~Erik and Nielsen, Kasper and Orlando, Ludovic and Sikora, Martin and Sjögren, Karl-Göran and Pedersen, Anders~Gorm and Schubert, Mikkel and Van~Dam, Alex and Kapel, Christian~Moliin~Outzen and Nielsen, Henrik~Bjørn and Brunak, Søren and Avetisyan, Pavel and Epimakhov, Andrey and Khalyapin, Mikhail~Viktorovich and Gnuni, Artak and Kriiska, Aivar and Lasak, Irena and Metspalu, Mait and Moiseyev, Vyacheslav and Gromov, Andrei and Pokutta, Dalia and Saag, Lehti and Varul, Liivi and Yepiskoposyan, Levon and Sicheritz-Pontén, Thomas and Foley, Robert~A. and Lahr, Marta~Mirazón and Nielsen, Rasmus and Kristiansen, Kristian and Willerslev, Eske},
  date = {2015-10-22},
  journaltitle = {Cell},
  shortjournal = {Cell},
