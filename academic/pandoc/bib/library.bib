  author = {Spyrou, Maria A. and Tukhbatova, Rezeda I. and Wang, Chuan-Chao and Valtueña, Aida Andrades and Lankapalli, Aditya K. and Kondrashin, Vitaly V. and Tsybin, Victor A. and Khokhlov, Aleksandr and Kühnert, Denise and Herbig, Alexander and Bos, Kirsten I. and Krause, Johannes},
