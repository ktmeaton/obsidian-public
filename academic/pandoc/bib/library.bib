@article{eckert1978BoundaryFormationDiffusion,
  title = {Boundary Formation and Diffusion of Plague : Swiss Epidemics from 1562 to 1669},
  shorttitle = {Boundary Formation and Diffusion of Plague},
  author = {Eckert, Edward A.},
